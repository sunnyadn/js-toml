{
  "name": "js-toml",
  "version": "0.1.1",
  "description": "A TOML parser for JavaScript/TypeScript, targeting TOML 1.0.0 Spec",
  "keywords": [
    "toml",
    "parser",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/sunnyadn/js-toml",
  "bugs": {
    "url": "https://github.com/sunnyadn/js-toml/issues",
    "email": "sunnyadn@foxmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Sunny Yang",
    "email": "sunnyadn@foxmail.com",
    "url": "https://www.linkedin.com/in/sunnyadn/"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunnyadn/js-toml.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "diagram": "esrun script/generateDiagram.ts",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "benchmark": "esrun benchmark/benchmark.ts"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.25",
    "@types/benchmark": "^2.1.3",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/coverage-v8": "^0.34.6",
    "benchmark": "^2.1.4",
    "esbuild": "^0.19.4",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "timezone-mock": "^1.3.4",
    "tsup": "^7.2.0",
    "typescript": "^4.8.4",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "xregexp": "^5.1.1"
  },
  "packageManager": "pnpm@8.9.0"
}
